<div class="container">
  <div class="row">
    <div class="col-md-9">
      <h1>Create a Universal Wishlist</h1>
    </div>

    <div class="col-md-9">
      <p>Explore new lists</p>
      <ul>
        <% List.all.each do |l| %>
          <li><%= link_to l.title, list_path(l) %></li>
        <% end %>
      </ul>
    </div>

  </div>
</div>